citHeader("To cite package 'QUIC' in publications use:")
if(!exists("meta") || is.null(meta)) meta <- packageDescription("QUIC")
     year <- sub(".*(2[[:digit:]]{3})-.*", "\\1", meta$Date)
     vers <- paste("R package version", meta$Version)
     
     citEntry(entry="incollection",
              title = "Sparse Inverse Covariance Matrix Estimation Using Quadratic Approximation",
              author = personList(as.person("Cho-Jui Hsieh"),
                                  as.person("Matyas A. Sustik"),
                                  as.person("Inderjit S. Dhillon"),
                                  as.person("Pradeep Ravikumar")),
	      booktitle = "Advances in Neural Information Processing Systems 24",
	      publisher = "http://nips.cc/",
              year = "2011",
	      editor = "J. Shawe-Taylor and R.S. Zemel and P. Bartlett and F.C.N. Pereira and K.Q. Weinberger",
	      pages = "2330--2338",
	      url = "http:\\www.cs.utexas.edu/users/sustik/QUIC",
              textVersion =
              paste("Sparse Inverse Covariance Matrix Estimation ",
	            "Using Quadratic Approximation, ",
		    "Cho-Jui Hsieh, Matyas A. Sustik,",
	            "Inderjit S. Dhillon, Pradeep Ravikumar, ",
                    "Advances in Neural Information Processing Systems 24, ",
		    "2011, p. 2330--2338. "))

